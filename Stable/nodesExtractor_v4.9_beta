ls --file-type *zip >> $datafile
Lines=$(cat $datafile)
count=1
for line in $Lines
do
	echo "$line"
	echo "$count | Unzipping $line file..."
	unzip $arg "$line" -d "$resdir"
	count=$(( $count+1 ))
	echo " "
done 
