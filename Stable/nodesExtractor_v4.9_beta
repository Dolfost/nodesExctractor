arg=$2
case "$1" in
-d|-n) echo "Loop #$countloops | Unzipping..."
       countloops=$(( $countloops + 1 ))
       echo " " ;;
-mnn|-mnd) echo "Loop #$countloops | Moving..."
           countloops=$(( $countloops + 1 ))
           echo  " " ;;
-cnn|-cnd) echo "Loop #$countloops | Coping..."
